2
#include <iostream>
using namespace std;
typedef unsigned short ushort;
void Line(ushort width, ushort height, char symbol) {
    for (ushort y = 0; y < height; y++) {
        for (ushort x = 0; x < width; x++) {
            cout << symbol;
        }
        cout << "\n";
    }
}
int main() {
    Line(5, 10, '@');
}
